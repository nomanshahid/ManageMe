<!-- Flip Dates Modal -->
<div class="modal fade" id="flipDates" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document" style="width:640px;">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Flip Dates</h4>
      </div>
      <div class="modal-body">
        <h4>Underage/Overage Flip Dates</h4>

        <table class="table table-condensed">
        <thead> 
          <tr>
            <th>Student Name</th>
            <% if current_user.admin? %>
              <th>Daycare Location</th>
            <% end %>
            <th>Current Age Group</th>
            <th>Flip Date</th>
            <th>Days Past/Early</th>
          </tr>
        </thead>

        <tbody>
        <% @kids.reorder('dob DESC').each do |kid| %>
        <% if kid.classroom == nil %>
        <% elsif !(kid.classroom.end_range == 72 || kid.classroom.end_range == 120) && (kid.flip_date_kid_query <= Date.today) %>
          <tr>
            <td><%= kid.first_name %> <%= kid.last_name %></td>
            <% if current_user.admin? %>
              <td><%= kid.user.daycare_location %></td>
            <% end %>
            <td><%= kid.classroom.class_name %></td>
            <td><%= kid.flip_date_kid %></td>
            <td style="color:red;"><%= (Date.today - kid.flip_date_kid_query).to_i %></td>
          </tr>
        <% elsif kid.classroom.start_range != 0 && (kid.upcoming_flip_date_calc_start > Date.today) %>
          <tr style="color:blue;">
            <td><%= kid.first_name %> <%= kid.last_name %></td>
            <% if current_user.admin? %>
              <td><%= kid.user.daycare_location %></td>
            <% end %>
            <td><%= kid.classroom.class_name %></td>
            <td><%= kid.upcoming_flip_date_calc_start.strftime("%B %d, %Y") %></td>
            <td><%= (kid.upcoming_flip_date_calc_start - Date.today).to_i %></td>
          </tr>
          <%end%>
          <% end %>
        </tbody>
        </table>

        <h4>Upcoming Flip Dates</h4>
        
        
        <table class="table table-condensed">
        <thead> 
          <tr>
            <th>Student Name</th>
            <% if current_user.admin? %>
              <th>Daycare Location</th>
            <% end %>
            <th>Current Age Group</th>
            <th>Flip Date</th>
            <th>Remaining Days</th>
          </tr>
        </thead>

        <tbody>
        <% @kids.order('dob DESC').each do |kid| %>
        <% if kid.upcoming_flip_date %>
          <tr>
            <td><%= kid.first_name %> <%= kid.last_name %></td>
            <% if current_user.admin? %>
              <td><%= kid.user.daycare_location %></td>
            <% end %>
            <td><%= kid.classroom.class_name %></td>
