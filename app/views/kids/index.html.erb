                <% @classrooms.each do |classroom| %>
                <tr data-id="<%= classroom.id %>">
                  <td><%= classroom.class_name %></td>
                  <% if current_user.admin? %>
                    <td><%= classroom.user.daycare_location %></td>
                  <% end %>
                  <td><%= classroom.kids.where(discharge_date: nil).count %>  /  <%= classroom.capacity %></td>
                  <td><%= classroom.kids.where(discharge_date: nil, student_id: "FF").count %></td>
                  <td><%= classroom.kids.where(discharge_date: nil, student_id: "CS").count %></td>
                  <td><%= classroom.teachers.count %></td>
                  <% if classroom.kids.where(discharge_date: nil).count >= classroom.capacity %>
                  <td style="color: red;">FULL</td>
                  <% else %>
                  <td style="color: green;">AVAILABLE</td>
                  <% end %>
                </tr>

                <!-- Show Class Modal -->
                <div class="modal fade" id="showClassModal<%=classroom.id %>" role="dialog">
                  <div class="modal-dialog">
                    <div class="modal-content">
                      <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4><%= classroom.class_name %></h4>
                      </div>
                      <div class="modal-body">  
                        <%= render 'classrooms/classmodal', classroom: classroom %>
                      </div>
                      <div class="modal-footer">
                      <% if Date.today.next_week.beginning_of_week.advance(days: 4).month != Date.today.next_week.beginning_of_week.month %>
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        <button class="btn btn-warning" onclick="window.location.href='/classrooms/<%=classroom.id%>/edit';">Edit</button>
                        <div class="btn-group" role="group">
                          <%= button_to 'Print 1', { controller: "kids", action: "print_attendance", id: classroom }, class: "btn btn-primary"%>
                          <%= button_to 'Print 2', { controller: "kids", action: "print_attendance_two", id: classroom }, class: "btn btn-primary"%>
                        </div>
                      <% else %>
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        <div style="float:right; margin-left: 7px;">
                          <%= button_to 'Print', { controller: "kids", action: "print_attendance", id: classroom }, class: "btn btn-primary"%>
                        </div>
                        <button class="btn btn-warning" onclick="window.location.href='/classrooms/<%=classroom.id%>/edit';">Edit</button>
                      <% end %>
                      </div>
                    </div>
                  </div>
                </div>

                <% end %>
              </tbody>
            </table>
          </div>  
        </div>
      </div>
    </div>
  </div>
</div>


